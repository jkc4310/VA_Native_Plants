@model IEnumerable<Plants>

<h2>Plants</h2>

<div style="display: block">
    <a href="/Plants/InsertPlant/">Create a New Plant</a>
</div>

<table class="table">
    <tr>
        <th>Type</th>
        <th>Common Name</th>
        <th>Scientific Name</th>
        <th>Region</th>
        <th>Uses</th>
        <th>Light</th>
        <th>Moisture</th>
        <th>Notes</th>
    </tr>

    @foreach (var plants in @Model) //removed Uri.EscapeDataString(@plants.CommonName) from first section. was causing null error
        {
        <tr>
            <td>@plants.Type</td>
            <td><a href="/Plants/ViewPlant/@plants.CommonName">@plants.CommonName</a></td> 
            <td>@plants.ScientificName</td>
            <td>@plants.Region</td>
            <td>@plants.Uses</td>
            <td>@plants.Light</td>
            <td>@plants.Moisture</td>
            <td>@plants.Notes</td>
        </tr>
    }

</table>
